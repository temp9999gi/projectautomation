<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE class SYSTEM "gen.dtd">
<class>
  <name>Account</name>
  <fields>
    <field>
      <database-field>d.user_name</database-field>
      <class-field>username</class-field>
      <type>String</type>
      <primary-key/>
    </field>
    <field>
      <database-field>d.dwarf_name</database-field>
      <class-field>dwarfName</class-field>
      <type>String</type>
      <insert-col/>
      <update-col/>
    </field>
    <field>
      <database-field>d.born</database-field>
      <class-field>born</class-field>
      <type>int</type>
      <insert-col/>
      <update-col/>
    </field>
    <field>
      <database-field>d.home_id</database-field>
      <class-field>homeId</class-field>
      <type>int</type>
      <insert-col/>
      <update-col/>
    </field>
    <field>
      <database-field>m.mountain_name</database-field>
      <class-field>homeName</class-field>
      <type>String</type>
    </field>
    <field>
      <database-field>d.spouse_id</database-field>
      <class-field>spouseId</class-field>
      <type>int</type>
    </field>
    <field>
      <database-field>s.dwarf_name</database-field>
      <class-field>spouseName</class-field>
      <type>String</type>
    </field>
  </fields>
  <primary-table>Account d</primary-table>
  <join-tables>
    <join-table>
      <join-name>Account s</join-name>
      <clause>d.spouse_id = s.dwarf_id</clause>
      <left-outer/>
    </join-table>
    <join-table>
      <join-name>mountain m</join-name>
      <clause>d.home_id = m.mountain_id</clause>
      <left-outer/>
    </join-table>
  </join-tables>
</class>

